local mats_ofltr:=mats->(dbfilter()),atemp:={},zfltr:=zakazch->(dbfilter())
priv stroki:={},pred:=0

pred:=(alert('формировать счёт-фактуру по:',{"ФАКТУ","ПРЕДОПЛАТЕ"})=2)

sele pudog
set order to 1
go top
seek ID_ZL9(DSCHET->DOG_ID)
set order to 2

sele dschet

aadd(atemp,'Услуги с-но дог. №'+alltrim(pudog->dog_no)+' от '+dtoc(pudog->dog_dt)+' г.'+iif(pred," (предоплата)","             "))
aadd(atemp,replicate(" ",30))
aadd(stroki,a2a(VarEdit("Предмет счёта",atemp),30))
 
if LastKey()!=27
 sele mats
 set filter to mats->sch_id=dschet->id .and. iif(pred,mats->psummad!=0,mats->summad!=0)
 count to n
 go top
 if n<1
  alert('Нет строк для распечатки!;;Возможно суммы на предоплату;равны нулю - необходимо исправить!')
 else
  do while !eof()
   sele ulist
   set filter to ulist->id=mats->mat_id
   go top
   ss:=alltrim(ulist->usluga)
   sele edizm
   set filter to edizm->id=ulist->ei_id
   go top
   ss1:=""
   sele mats
   if lena(mats->comment1)>0
    ss1=ss1+" "+alltrim(mats->comment1)
   endif
   if lena(mats->comment2)>0
    ss1=ss1+" "+alltrim(mats->comment2)
   endif
   if lena(mats->comment3)>0
    ss1=ss1+" "+alltrim(mats->comment3)
   endif
 
   ss1:=alltrim(ss1)
   if lena(ss1)>0
    ss1:="("+ss1+")"
   endif 
 
   aadd(stroki,{ss+" "+ss1,;
               strcent(alltrim(edizm->edizm+' '),5),;
               mats->kolvo,;
               mats->cena,;
               mats->summad,;
               mats->pNDS})
 
   if lena(mats->comment1)>0 .or. lena(mats->comment2)>0 .or. lena(mats->comment3)>0
    aadd(stroki,{nleft("  ",57),"     ",0,0,0,0,0})
   endif
   skip
  enddo
  sele zakazch
  set filter to zakazch->id=pudog->zak_id
  go top  
  fschetprint(.t.) 
  sele zakazch
  set filter to &zfltr
 endif

 sele mats
 set filter to &mats_ofltr
 goto mats_orn

 sele dschet
 set filter to &ds_ofltr
 goto ds_orn

 sele pudog
 set filter to &pd_ofltr
 goto pd_orn

endif 

retu
























